Screen:

    MDTopAppBar:
        id: toolbar
        pos_hint: {"top": 1}
        elevation: 5
        title: 'Лучший предмет в мире'
        left_action_items: [["menu", lambda x: nav_drawer.set_state("open")]]

    MDNavigationLayout:
        x: toolbar.height

        ScreenManager:
            id: screen_manager

            Screen:
                name: "main"
                MDBoxLayout:
                    adaptive_height: True
                    md_bg_color: app.theme_cls.primary_color
                    MDLabel:
                        text: "Представься, чтобы мы знали, кто плохо знает математику"
                        halign: "center"

                    MDTextField:
                        id: uname
                        hint_text: "Tikni suda"
                        helper_text: "Vvedi svou familiu"
                        helper_text_mode: "on_focus"
                        text_color: 0, 0, 1, 1
                    MDRaisedButton:
                        id: button_1_q
                        text: "Далее"
                        text_color: 0, 0, 1, 1
                        on_press:
                            app.uname(uname.text)

            Screen:
                name: "scr 1"
                MDBoxLayout:
                    adaptive_height: True
                    md_bg_color: app.theme_cls.primary_color
                    MDLabel:
                        text: 'Вопросий: "1+1=?"'
                        halign: "center"
                    MDTextField:
                        id: scr_test_1
                        hint_text: "TYT tvoi otvet"
                        helper_text: "Vvedi svoi otvet"
                        helper_text_mode: "on_focus"
                    MDRaisedButton:
                        id: button_1_q
                        text: "Далее"
                        text_color: 0, 0, 1, 1
                        on_press:
                            app.submit_1(scr_test_1.text)
